{
  "name": "ideabox",
  "version": "1.0.1",
  "description": "IdeaBox module",
  "dnn": {
    "projectFolders": [
      "Server/IdeaBox",
      "Server/Core"
    ],
    "owner": {
      "name": "Peter Donker",
      "organization": "DNN Connect",
      "url": "www.bring2mind.net",
      "email": "peter@bring2mind.net"
    },
    "pathsAndFiles": {
      "solutionFile": "./Connect.IdeaBox.sln",
      "pathToAssemblies": "./bin",
      "devSiteUrl": "",
      "devSitePath": "",
      "excludeFilter": [
        "bin/",
        "obj/",
        "node_modules"
      ],
      "licenseFile": "./License.md",
      "releaseNotesFile": "",
      "releaseFiles": [
        "App_LocalResources/*.resx",
        "**/*.ascx",
        "**/*.cshtml",
        "**/*.vbhtml",
        "js/*.js",
        "css/*.css",
        "*.css",
        "**/*.html",
        "**/*.png",
        "**/*.jpg",
        "**/*.jpeg",
        "**/*.gif",
        "**/*.eot",
        "**/*.svg",
        "**/*.ttf",
        "**/*.woff",
        "**/*.woff2",
        "*.txt"
      ],
      "zipName": "Connect.IdeaBox"
    }
  },
  "scripts": {
    "build": "set NODE_ENV=production&&webpack --config Client/webpack.config.js",
    "watch": "webpack --config Client/webpack.config.js --progress --watch",
    "watch-server": "node watch.js"
  },
  "author": "Peter Donker",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jquery": "^3.5.14",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "css-loader": "^6.7.1",
    "deepmerge": "^4.2.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
